<h1> Pictures (<%= @pictures.length %>)</h1>

<% if current_user %>
	<%= link_to "Add a New Picture", "/pictures/new" %>
<% end %>

<% @pictures.each do |picture| %>
	<p> <%= picture[:title] %>, <%= picture[:artist] %> </p>

	<%= link_to image_tag(picture.image.thumb.url), "/pictures/#{picture.id}" %>

<% end %>

